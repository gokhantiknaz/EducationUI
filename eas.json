{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.oakacademy.education"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.oakacademy.education"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.oakacademy.education"
      }
    },
    "medical-training": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.company.medical"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "safety-training": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.oakacademy.education"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "corporate-training": {
      "extends": "production",
      "env": {
        "EXPO_PUBLIC_APP_ID": "com.oakacademy.education"
      },
      "android": {
        "buildType": "apk"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
